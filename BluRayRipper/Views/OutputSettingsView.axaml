<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="clr-namespace:BluRayRipper.ViewModels"
             xmlns:utils="clr-namespace:BluRayRipper.Utils"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="BluRayRipper.Views.OutputSettingsView"
             x:DataType="vm:OutputSettingsViewModel"
             Design.DataContext="{utils:DesignResolve {x:Type vm:OutputSettingsViewModel}}">
    
    <Border Padding="10">
        <StackPanel Orientation="Horizontal" Spacing="10">
            <Button Command="{Binding StartQueue}">Start queue</Button>
            <Button Command="{Binding RefreshAsync}">Refresh</Button>
        </StackPanel>
    </Border>
    
</UserControl>
